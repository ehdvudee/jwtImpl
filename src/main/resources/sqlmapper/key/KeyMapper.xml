<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org/DTD Mapper 3.0// EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
  
<mapper namespace = "net.glaso.mapper.key">
    <select id="selectKeyPairInfoLastOne" resultType="keyPairVo">
        select
            seq_id seqId,
            pri_key priKey,
            cert,
            usage,
            kid
        from
            key_pair_info
        where
            usage = true
        order by
            seq_id desc
        limit 1;
    </select>

</mapper>